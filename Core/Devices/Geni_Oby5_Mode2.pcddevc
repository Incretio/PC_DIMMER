  unit GeniOby5;

  interface

  procedure InitDevice(Device: String);
  procedure FormShow;
  procedure FormRefresh(channel:Integer);
  procedure FormClose;
  procedure PositionXYChange(Top, Left: Integer);
  procedure ColorBoxChange;
  procedure dimmersliderchange;
  procedure shuttersliderchange;
  procedure ColorBoxChange2;
  procedure gobo1boxchange;
  procedure gobo2boxchange;
  procedure gobo2anglesliderchange;
  procedure gobo2rotsliderchange;
  procedure prismasliderchange;
  procedure irisadjustmentsliderchange;
  procedure irisinoutsliderchange;
