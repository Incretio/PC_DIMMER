  procedure lamponbtnclick;
  procedure lampoffbtnclick;
  procedure resetbtnclick;
  procedure controlboxchange;

  implementation

  var
    dontrefresh:boolean;

  procedure InitDevice(Device: String);
  begin
    // diese Funktion wird für alle Geräte diesen Typs beim ersten Start des PC_DIMMERs ausgeführt.
  end;
  procedure FormShow;
  begin
    // diese Funktion wird bei jedem Laden der Form ausgeführt.
  end;
  procedure FormRefresh(channel:Integer);
	var
		wert:integer;
		r,phi,temp:single;
  begin
		if not dontrefresh then
    case channel of
    	0:
    	begin
				dimmerslider.position:=get_channel('dimmer');
    	end;
    	1:
    	begin
    		wert:=get_channel('shutter');
    		case wert of
    			0: shutterbox.itemindex:=0;
    			1..7: shutterbox.itemindex:=1;
					8..63:
					begin
						shutterbox.itemindex:=2;
						shutterslider.position:=wert;
					end;
    			64..71: shutterbox.itemindex:=1;
					72..127:
					begin
						shutterbox.itemindex:=3;
						shutterslider.position:=wert-64;
					end;
    			128..135: shutterbox.itemindex:=1;
					136..191:
					begin
						shutterbox.itemindex:=3;
						shutterslider.position:=wert-128;
					end;
    			192..199: shutterbox.itemindex:=1;
					200..255:
					begin
						shutterbox.itemindex:=4;
						shutterslider.position:=wert-192;
					end;
    		end;
    	end;
    	2:
    	begin
    		wert:=get_channel('color1');
    		case wert of
    			0..13: colorbox.itemindex:=0;
    			14..27: colorbox.itemindex:=1;
    			28..41: colorbox.itemindex:=2;
    			42..55: colorbox.itemindex:=3;
    			56..69: colorbox.itemindex:=4;
    			70..83: colorbox.itemindex:=5;
    			84..97: colorbox.itemindex:=6;
    			98..111: colorbox.itemindex:=7;
    			112..125: colorbox.itemindex:=8;
    			126..139: colorbox.itemindex:=9;
    			140..153: colorbox.itemindex:=10;
    			154..167: colorbox.itemindex:=11;
    		end;
    	end;
    	3:
    	begin
    		wert:=get_channel('color2');
    		case wert of
    			0..13: colorbox2.itemindex:=0;
    			14..27: colorbox2.itemindex:=1;
    			28..41: colorbox2.itemindex:=2;
    			42..55: colorbox2.itemindex:=3;
    			56..69: colorbox2.itemindex:=4;
    			70..83: colorbox2.itemindex:=5;
    			84..97: colorbox2.itemindex:=6;
    			98..111: colorbox2.itemindex:=7;
    			112..125: colorbox2.itemindex:=8;
    			126..139: colorbox2.itemindex:=9;
    			140..153: colorbox2.itemindex:=10;
    			154..167: colorbox2.itemindex:=11;
   			end;
    	end;
    	4:
    	begin
    		wert:=get_channel('gobo1');
    		case wert of
    			0..15: gobo1box.itemindex:=0;
    			16..31: gobo1box.itemindex:=1;
    			32..47: gobo1box.itemindex:=2;
    			48..63: gobo1box.itemindex:=3;
    			64..79: gobo1box.itemindex:=4;
    			80..95: gobo1box.itemindex:=5;
    			96..111: gobo1box.itemindex:=6;
    			112..127: gobo1box.itemindex:=7;
    			128..143: gobo1box.itemindex:=8;
    			144..159: gobo1box.itemindex:=9;
   			end;
    	end;
    	5:
    	begin
    		wert:=get_channel('gobo2');
    		case wert of
    			0..23: gobo2box.itemindex:=0;
    			24..47: gobo2box.itemindex:=1;
    			48..71: gobo2box.itemindex:=2;
    			72..95: gobo2box.itemindex:=3;
    			96..119: gobo2box.itemindex:=4;
    			120..143: gobo2box.itemindex:=5;
    			144..167: gobo2box.itemindex:=6;
   			end;
    	end;
    	6:
    	begin
    		wert:=get_channel('gobo2rot');
    		case wert of
    			0..127:
    			begin
    				gobo2angleslider.position:=wert;
    			end;
    			128..255:
    			begin
    				gobo2rotslider.position:=wert;
    			end;
    		end;
    	end;
    	7:
    	begin
    		wert:=get_channel('prisma');
    		case wert of
    			0..1:
    			begin
    				prismabox.itemindex:=0;
    			end;
    			2..7:
    			begin
    				prismabox.itemindex:=1;
    			end;
    			8..253:
    			begin
    				prismabox.itemindex:=2;
    				prismaslider.position:=wert;
    			end;
    			254..255:
    			begin
    				prismabox.itemindex:=0;
    			end;
    		end;
    	end;
    	8:
    	begin
    		wert:=get_channel('iris');
    		case wert of
    			0..127: irisadjustmentslider.position:=wert;
    			128..255: irisinoutslider.position:=wert;
//				254..255: irisinoutslider.position:=127;
    		end;
    	end;
    	9:
    	begin
				focusslider.position:=get_channel('focus');
    	end;
    	10..13:
    	begin
			if usemhcontrol.checked then
			begin
				// Moving-Head-Steuerung
				temp:=get_channel('tilt')+(get_channel('tiltfine')/256);
				case round(temp) of
					0..127: r:=((127-temp)/127);
					128..255: r:=((temp-128)/127);
				end;
				phi:=(get_channel('pan')+(get_channel('panfine')/256))/255*6.283185307179586476925286766559;	//(X/255)*PI
				PositionXY.Left:=round(r*sin(phi)*(Fadenkreuz.Width/2)+(Fadenkreuz.Width/2)-(PositionXY.Width div 2));
				PositionXY.Top:=round(r*cos(phi)*(Fadenkreuz.Height/2)+(Fadenkreuz.Height/2)-(PositionXY.Width div 2));
			end else
			begin
				// Scanner-Steuerung
				PositionXY.Left:=round(Fadenkreuz.Width * (get_channel('pan')+get_channel('panfine')/256)/255)-(PositionXY.Width div 2);
				PositionXY.Top:=round(Fadenkreuz.Height * (get_channel('tilt')+get_channel('tiltfine')/256)/255)-(PositionXY.Height div 2);
			end;
    	end;
    	14:
    	begin
    		wert:=get_channel('special1');
    		case wert of
					0..7: controlbox.itemindex:=0;
					8..31: controlbox.itemindex:=1;
					32..45: controlbox.itemindex:=2;
					46..63: controlbox.itemindex:=3;
    		end;
    	end;
    	15:
    	begin
    		//
    	end;
    end;
	dontrefresh:=false;
  end;
  procedure FormClose;
  begin
    // diese Funktion wird bei jedem Schließen der Form ausgeführt.
  end;
  procedure PositionXYChange(Top, Left: Integer);
  var
	phi,r,x,y:single;
	pan, tilt, panfine, tiltfine:single;
  begin
	if usemhcontrol.checked then
	begin
		// Moving-Head-Steuerung (Polarkoordinaten)
		x:=((PositionXY.Left+(PositionXY.Width div 2)-(fadenkreuz.Width/2))/fadenkreuz.Width)*2;	//-1..0..1
		y:=((PositionXY.Top+(PositionXY.Height div 2)-(fadenkreuz.Width/2))/fadenkreuz.Height)*2;	//-1..0..1
		r:=sqrt(x*x+y*y);
	
		if (r>0) then
		begin
			if (y>=0) then
				phi:=arccos(x/r)
			else
				phi:=6.283185307179586476925286766559-arccos(x/r);
		end else
			phi:=64;
		
		r:=128-((r/2)*255);
		phi:=(phi/6.283185307179586476925286766559)*255;
		if 64>=phi then
			phi:=phi+191
		else
			phi:=phi-64;
	
		pan:=255-phi;
		tilt:=255-r;
	end else
	begin
		// Scannersteuerung (Kartesische Koordinaten)
		pan:=((PositionXY.Left+(PositionXY.Width div 2))/fadenkreuz.Width)*255;
		tilt:=((PositionXY.Top+(PositionXY.Height div 2))/fadenkreuz.Height)*255;
	end;
	
	// Ausgabe
	if panmirror.checked then
	begin
		dontrefresh:=true;
		set_channel('pan',trunc(255-pan),trunc(255-pan),0);
		dontrefresh:=true;
		set_channel('panfine',trunc(frac(255-pan)*255),trunc(frac(255-pan)*255),0);
	end	else
	begin
		dontrefresh:=true;
		set_channel('pan',trunc(pan),trunc(pan),0);
		dontrefresh:=true;
		set_channel('panfine',trunc(frac(pan)*255),trunc(frac(pan)*255),0);
	end;
	if tiltmirror.checked then
	begin
		dontrefresh:=true;
		set_channel('tilt',trunc(255-tilt),trunc(255-tilt),0);
		dontrefresh:=true;
		set_channel('tiltfine',trunc(frac(255-tilt)*255),trunc(frac(255-tilt)*255),0);
	end	else
	begin
		dontrefresh:=true;
		set_channel('tilt',trunc(tilt),trunc(tilt),0);
		dontrefresh:=true;
		set_channel('tiltfine',trunc(frac(tilt)*255),trunc(frac(tilt)*255),0);
	end;
  end;
  procedure ColorBoxChange;
  begin
		case colorbox.itemindex of
			0: set_channel('color1', 0, 0, 0);
			1: set_channel('color1', 14, 14, 0);
			2: set_channel('color1', 28, 28, 0);
			3: set_channel('color1', 42, 42, 0);
			4: set_channel('color1', 56, 56, 0);
			5: set_channel('color1', 70, 70, 0);
			6: set_channel('color1', 84, 84, 0);
			7: set_channel('color1', 98, 98, 0);
			8: set_channel('color1', 112, 112, 0);
